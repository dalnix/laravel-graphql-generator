{
  "name": "dalnix/laravel-graphql-generator",
  "description": "Generator of Queries, Types and Mutations for laravel-graphql",
  "keywords": [
    "framework",
    "laravel",
    "graphql",
    "generator"
  ],
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Dalnix AB",
      "email": "info@dalnix.se",
      "homepage": "https://dalnix.se"
    },
    {
      "name": "Christian Janbjer",
      "email": "christian@dalnix.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "laravel/framework": "~5.6.0|~5.7.0",
    "folklore/graphql": "~1"
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dalnix\\GraphQLGenerator\\GraphQLGeneratorProvider"
      ]
    }
  },
  "autoload": {
    "psr-0": {
      "Dalnix\\GraphQLGenerator\\": "src/"
    }
  },
  "minimum-stability": "dev"
}
